<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>sqlcon</id>
    <version>1.7.9.0</version>
    <title>sqlcon</title>
    <authors>datconn</authors>
    <licenseUrl>http://tie.codeplex.com/license</licenseUrl>
    <projectUrl>https://github.com/fjiang2/sqlcon/</projectUrl>
    <iconUrl>http://download-codeplex.sec.s-msft.com/Download?ProjectName=tie&amp;DownloadId=331680&amp;Build=20841</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>
      Operate databases(servers) like file system on the disk
      sqlcon command: dir,cd,mk,del,type,ren,copy,comp,xcopy,cls,ver,echo,rem,set,import,export,mount,umount,open,save,edit,clean,execute
      Support SQL Server, LocalDB, SQL Server Azure, SQL Server Express
      Support Xml database on disk, web site and ftp site
      Support unlimited databse connections
      Compare and copy data/schema between 2+ databases or database servers
      Create/Search/Delete data and schema
      Edit data on GUI or console
      Clean redundant rows
      Support all SQL statements
      Generate C# class: data contract, Linq to SQL, and entity framework
      Export json data,CSV,
      Generate SQL clauses
    </description>
    <summary> SQL Console Tools for DevOps</summary>
    <releaseNotes>
      03/15/2017 v1.7.9.0
      New Features:
      1. first release to nuget
    </releaseNotes>
    <dependencies>
      <group targetFramework=".NETFramework4.5">
        <dependency id="Tie" version="2.0.5.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\sqlcon\bin\Release\sqlcon.exe" target="lib\net45\sqlcon.exe" />
    <file src="..\sqlcon\bin\Release\sqlcon.cfg" target="lib\net45\sqlcon.cfg" />
    <file src="..\sqlcon\bin\Release\syscore.dll" target="lib\net45\syscore.dll" />
    <file src="..\sqlcon\bin\Release\Tie.dll" target="lib\net45\Tie.dll" />
  </files>
</package>